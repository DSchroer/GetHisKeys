            <kind>1</kind>
            <string>if(item_index != noone)
{
    if(movement_target != item_index)
    {
        //path_reset(follower)
        //follow_create(self,item_index,16)
        movement_target = item_index;
    }
}else{
///State Machine
    if(state == "drink")
    {
        if(!movement_target || movement_target.object_index != obj_drink_area)
        {
            scr_find_area(obj_drink_area);
        }else if(!transit)
        {
            image_angle = movement_target.image_angle
        }
    }if(state == "pee")
    {
        if(!movement_target || movement_target.object_index != obj_bathroom_area)
        {
            scr_find_area(obj_bathroom_area);
        }else if(!transit)
        {
            image_angle = movement_target.image_angle
        }
    }if(state == "party")
    {
        if(!movement_target || movement_target.object_index != obj_party_area)
        {
            scr_find_area(obj_party_area);
        }else if(!transit)
        {
            state = "drink";
        }
    }
}

if(movement_target != prev_target)
{
    path_reset(self)
    follow_create(self,movement_target,16)
    prev_target = movement_target;
    transit = true;
}

follow_update(100)

if(point_distance(x,y,nx,ny) &lt;= 1)
{
    if(follow_has_point())
    {
        x = nx;
        y = ny;
        var pt = follow_get_point();
        nx = pt.x;
        ny = pt.y;
    }else{
        //Item Pickup
        var obj = collision_circle(x,y,7,item_index,true,true);
        if(obj != noone)
        {
            item_sprite = obj.sprite_index;
            item_ref = obj.object_index;
            item_index = noone;
            with(obj)
            {
                instance_destroy()
            }
        }
        
        //State Transition
        obj = collision_circle(x,y,7,movement_target,true,true);
        if(obj != noone)
        {
            transit = false;
        }
        
        
        speed = 0;
    }
}else{
    move_towards_point(nx,ny,2)
    image_angle = point_direction(x,y,nx,ny)
}

    


